nome_curso = 'Edição de Vídeo'

print(nome_curso.upper()) # DEIXA TUDO MAIUSCULO
print(nome_curso.lower()) # deixa tudo minusculo
print(nome_curso.strip()) # Remove espaços em branco
print(nome_curso.lstrip()) # Remove espaços em branco a esquerda
print(nome_curso.rstrip()) # Remove espaços em branco a direita
print(nome_curso.find('ção')) # Procura a letra na string e retorna o índice
print(nome_curso.replace('Vídeo', 'Música')) # Substitui a palavra desejada na String
print('https://www.olx.com.br/autos-e-pecas/carros-vans-e-utilitarios'.replace('carros', 'motos')) # Substitui a palavra desejada na String